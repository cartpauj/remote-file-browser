{
	"name": "remote-file-browser",
	"displayName": "Remote File Browser",
	"description": "Browse remote files via SFTP/FTP without downloading them locally",
	"version": "1.0.0",
	"publisher": "cartpauj",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/cartpauj/remote-file-browser.git"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Remote Development"
	],
	"license": "GPL-3.0",
	"author": "cartpauj",
	"homepage": "https://github.com/cartpauj/remote-file-browser",
	"bugs": {
		"url": "https://github.com/cartpauj/remote-file-browser/issues"
	},
	"keywords": [
		"sftp",
		"ftp",
		"remote",
		"ssh",
		"files",
		"browser",
		"server",
		"file-manager",
		"remote-development",
		"file-explorer",
		"secure",
		"connection",
		"sync",
		"file-transfer",
		"remote-files",
		"ssh-client",
		"file-system",
		"network",
		"remote-server",
		"file-browser"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "remoteFileBrowser.connect",
				"title": "Connect to Remote Server",
				"category": "Remote File Browser"
			},
			{
				"command": "remoteFileBrowser.connectFromConfig",
				"title": "Connect from Saved Configuration",
				"category": "Remote File Browser"
			},
			{
				"command": "remoteFileBrowser.manageConnections",
				"title": "Manage Connections",
				"category": "Remote File Browser"
			},
			{
				"command": "remoteFileBrowser.disconnect",
				"title": "Disconnect from Remote Server",
				"category": "Remote File Browser",
				"icon": "$(close)"
			},
			{
				"command": "remoteFileBrowser.refresh",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "remoteFileBrowser.cleanupTempFiles",
				"title": "Clean Up Temporary Files",
				"category": "Remote File Browser",
				"icon": "$(trash)"
			},
			{
				"command": "remoteFileBrowser.connectFromWelcome",
				"title": "Connect from Welcome",
				"category": "Remote File Browser"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "remoteFileBrowser",
					"title": "Remote Files",
					"icon": "$(globe)"
				}
			]
		},
		"views": {
			"remoteFileBrowser": [
				{
					"id": "remoteFilesWelcome",
					"name": "Connection",
					"when": "!remoteFileBrowser.connected"
				},
				{
					"id": "remoteFilesList",
					"name": "Remote Files",
					"when": "remoteFileBrowser.connected"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "remoteFileBrowser.refresh",
					"when": "view == remoteFilesList",
					"group": "navigation"
				},
				{
					"command": "remoteFileBrowser.disconnect",
					"when": "view == remoteFilesList",
					"group": "navigation"
				},
				{
					"command": "remoteFileBrowser.cleanupTempFiles",
					"when": "view == remoteFilesList",
					"group": "navigation"
				}
			],
			"commandPalette": [
				{
					"command": "remoteFileBrowser.connect"
				},
				{
					"command": "remoteFileBrowser.connectFromConfig"
				},
				{
					"command": "remoteFileBrowser.manageConnections"
				},
				{
					"command": "remoteFileBrowser.disconnect",
					"when": "remoteFileBrowser.connected"
				},
				{
					"command": "remoteFileBrowser.cleanupTempFiles"
				},
				{
					"command": "remoteFileBrowser.connectFromWelcome"
				}
			]
		},
		"configuration": {
			"title": "Remote File Browser",
			"properties": {
				"remoteFileBrowser.connections": {
					"type": "array",
					"default": [],
					"description": "Saved remote server connections",
					"items": {
						"type": "object",
						"properties": {
							"name": {
								"type": "string",
								"description": "Connection name"
							},
							"protocol": {
								"type": "string",
								"enum": [
									"sftp",
									"ftp"
								],
								"description": "Protocol to use"
							},
							"host": {
								"type": "string",
								"description": "Server hostname or IP"
							},
							"port": {
								"type": "number",
								"description": "Server port"
							},
							"username": {
								"type": "string",
								"description": "Username"
							},
							"remotePath": {
								"type": "string",
								"default": "/",
								"description": "Remote path to browse"
							},
							"authType": {
								"type": "string",
								"enum": [
									"password",
									"key"
								],
								"default": "password",
								"description": "Authentication method"
							},
							"keyPath": {
								"type": "string",
								"description": "Path to SSH private key file"
							},
							"passphrase": {
								"type": "string",
								"description": "Passphrase for encrypted SSH key (optional)"
							}
						}
					}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out"
	},
	"devDependencies": {
		"@types/node": "16.x",
		"@types/ssh2-sftp-client": "^9.0.4",
		"@types/vscode": "^1.74.0",
		"node-loader": "^2.1.0",
		"ts-loader": "^9.5.2",
		"typescript": "^4.9.4",
		"webpack": "^5.99.9",
		"webpack-cli": "^6.0.1"
	},
	"dependencies": {
		"basic-ftp": "^5.0.3",
		"ssh2-sftp-client": "^9.1.0"
	},
	"__metadata": {
		"size": 89222892
	}
}
